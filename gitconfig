[core]
	autocrlf = false
	editor = vim
	excludesfile = ~/.config/git/gitignore_global
[commit]
	verbose = true
	gpgSign = false
[merge]
	ff = false
[pull]
	ff = only
[alias]
	a = add -v
	aa = add -vA
	ai = add -i
	aliases = config --get-regexp alias.*
	b = branch
	c = commit
	cm = commit -m
	co = !echo use 'switch' or 'restore' instead of co \\(checkout\\) && false
	f = fetch
	fu = fetch upstream
	graph = log --graph --date=short --decorate=full
	ps = push
	psu = !git push -u origin `git symbolic-ref --short HEAD`
	pl = pull
	s = status
	df = diff
	diffstgd = diff --staged
	dfst = diff --staged
	user = "!git config user.name; git config user.email"
	sw = switch
[user]
	name = Ryooooooga
	email = ryoga_314@yahoo.co.jp
	signingKey = ""
[gpg]
	program = gpg
[tag]
	forceSignAnnotated = false
